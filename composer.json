{
    "name": "whatis/ozon-seller",
    "version": "0.0.1",
    "description": "Библиотека для взаимодействия с wildberries api",
    "type": "library",
    "keywords": [
        "whatis",
        "ozon-seller",
        "ozon",
        "seller",
        "php",
        "api",
        "client"
    ],
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.8"
    },
    "require-dev": {
        "php": "^8.2",
        "saggre/phpdocumentor-markdown": "^0.1.3"
    },
    "license": "unlicense",
    "autoload": {
        "psr-4": {
            "Whatis\\OzonSeller\\": "src/"
        },
        "files": ["src/register/registerServices.php"]
    },
    "authors": [
        {
            "name": "Whatis",
            "email": "anton-gogo@mail.ru",
            "role": "Developer"
        }
    ],
    "scripts": {
        "phpdoc-html": [
            "phpdoc -d src -t docs/html"
        ],
        "phpdoc-markdown": [
            "phpdoc -d src -t docs/markdown --template=vendor/saggre/phpdocumentor-markdown/themes/markdown"
        ],
        "phpdoc": [ "@phpdoc-html", "@phpdoc-markdown" ]
    }
}
